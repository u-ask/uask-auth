{
  "name": "uask-auth",
  "version": "1.1.1",
  "description": "uask authentication",
  "exports": {
    ".": "./dist/server/index.js",
    "./client": "./dist/client/index.js",
    "./example": "./dist/server/example.js"
  },
  "types": "./dist/server/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "dist/client/index.d.ts"
      ],
      "example": [
        "dist/server/example.d.ts"
      ]
    }
  },
  "type": "module",
  "files": [
    "dist",
    "db/migrations",
    "db/seeds"
  ],
  "scripts": {
    "build": "npm run build-server; npm run build-client",
    "build-client": "./node_modules/.bin/rollup -c ./rollup.client.js",
    "build-server": "./node_modules/.bin/rollup -c ./rollup.server.js",
    "serve": "APP_ENV=development nodemon -r dotenv/config src/devserver.js",
    "test": "node ./test-runner.js",
    "lint": "./node_modules/.bin/eslint . --ext .js",
    "migrate": "npm run knex migrate:latest",
    "seed": "npm run knex seed:run",
    "knex": "NODE_ENV=$APP_ENV ./node_modules/.bin/knex"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/u-ask/uask-auth.git"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/u-ask/uask-auth#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "debug": "^4.3.4",
    "ejs": "^3.1.8",
    "knex": "^2.1.0",
    "oidc-provider": "^7.11.2",
    "oidc-provider-knex-adapter": "^1.2.0",
    "open": "^8.4.0",
    "openid-client": "^5.1.6",
    "pg": "^8.7.3",
    "restana": "^4.9.5",
    "stealer": "^1.0.6",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/knex": "^0.16.1",
    "@wabson/list-dependencies": "^0.1.0",
    "chrome-launcher": "^0.15.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "got": "^12.1.0",
    "has-property-descriptors": "^1.0.0",
    "jwt-decode": "^3.1.2",
    "lowdb": "^3.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "puppeteer-core": "^14.1.1",
    "rewire": "^6.0.0",
    "rollup": "^2.74.1",
    "rollup-plugin-copy": "^3.4.0",
    "serve-static": "^1.15.0",
    "sinon": "^14.0.0",
    "sqlite3": "^5.0.8",
    "tape": "^5.5.3",
    "typescript": "^4.7.2",
    "uuid": "^8.3.2"
  }
}
